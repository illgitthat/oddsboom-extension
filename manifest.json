{
  "name": "Oddsboom",
  "version": "0.1",
  "manifest_version": 3,
  "description": "Boom",
  "web_accessible_resources": [{
    "resources": ["inject.js"],
    "matches": ["https://*.williamhill.com/*"]
}],
"content_scripts": [{
  "js": ["content_script.js"],
  "run_at": "document_start",
  "matches": ["https://*.williamhill.com/*"]
}],
  "permissions": [
    "tabs"
  ]
}
