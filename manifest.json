{
  "name": "Oddsboom - Sportsbooks Tweaks",
  "version": "1.0",
  "manifest_version": 3,
  "description": "Automatically adjusts stake on Caesar's for limited accounts.",
  "web_accessible_resources": [{
    "resources": ["inject.js"],
    "matches": ["https://*.williamhill.com/*"]
}],
"content_scripts": [{
  "js": ["content_script.js"],
  "run_at": "document_start",
  "matches": ["https://*.williamhill.com/*"]
}],
  "permissions": [
    "tabs"
  ]
}
